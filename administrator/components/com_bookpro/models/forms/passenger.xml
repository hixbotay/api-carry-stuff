<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset addfieldpath="/administrator/components/com_bookpro/elements">
		<fields>

			<field name="id" type="hidden" default="0" required="true"
				readonly="true" />

			<field id="title" name="title" type="text" required="false"
				label="COM_BOOKPRO_SELECT_TITLE" description="COM_BOOKPRO_SELECT_TITLE" class="inputbox" size="40" />


			<field id="firstname" name="firstname" type="text" required="true"
				label="COM_BOOKPRO_PASSENGER_FIRSTNAME" description="COM_BOOKPRO_PASSENGER_FIRSTNAME" class="inputbox"
				size="40" />


			<field id="lastname" name="lastname" type="text" required="true"
				label="COM_BOOKPRO_PASSENGER_LASTNAME" description="COM_BOOKPRO_PASSENGER_LASTNAME" class="inputbox" size="40" />


			<field id="gender" name="gender" type="list" required="false"
				label="COM_BOOKPRO_PASSENGER_GENDER" description="COM_BOOKPRO_PASSENGER_GENDER" size="1">
				<option value="M">COM_BOOKPRO_MALE</option>
				<option value="F">COM_BOOKPRO_FEMALE</option>
			</field>


			<field id="passport" name="passport" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_PASSPORT" description="COM_BOOKPRO_PASSENGER_PASSPORT" class="inputbox" size="40" />


			<field id="ppvalid" name="ppvalid" type="calendar" required="false"
				label="COM_BOOKPRO_PASSENGER_PPVALID" description="COM_BOOKPRO_PASSENGER_PPVALID" class="inputbox" size="16"
				format="%Y-%m-%d" />


			<field id="birthday" name="birthday" type="calendar" required="false"
				label="COM_BOOKPRO_PASSENGER_BIRTHDAY" description="COM_BOOKPRO_PASSENGER_BIRTHDAY" class="inputbox" size="16"
				format="%Y-%m-%d" />


			<field id="customer_id" name="customer_id" type="text"
				required="false" label="COM_BOOKPRO_PASSENGER_CUSTOMER_ID" description="COM_BOOKPRO_PASSENGER_CUSTOMER_ID"
				class="inputbox" size="40" />


			<field id="order_id" name="order_id" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_ORDER_ID" description="COM_BOOKPRO_PASSENGER_ORDER_ID" class="inputbox" size="40" />


			<field id="orderinfo_id" name="orderinfo_id" type="text"
				required="false" label="COM_BOOKPRO_PASSENGER_ORDERINFO_ID" description="COM_BOOKPRO_PASSENGER_ORDERINFO_ID"
				class="inputbox" size="40" />


			<field id="country_id" name="country_id" type="country"
				required="false" label="COM_BOOKPRO_PASSENGER_COUNTRY" description="COM_BOOKPRO_PASSENGER_COUNTRY"
				class="inputbox" size="40" />


			<field id="group_id" name="group_id" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_GROUP_ID" description="COM_BOOKPRO_PASSENGER_GROUP_ID" class="inputbox" size="40" />


			<field id="documenttype" name="documenttype" type="text"
				required="false" label="COM_BOOKPRO_PASSENGER_DOCUMENTTYPE" description="COM_BOOKPRO_PASSENGER_DOCUMENTTYPE"
				class="inputbox" size="40" />


			<field id="seat" name="seat" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_SEAT" description="COM_BOOKPRO_PASSENGER_SEAT" class="inputbox" size="40" />


			<field id="return_seat" name="return_seat" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURNSEAT" description="COM_BOOKPRO_PASSENGER_RETURNSEAT" class="inputbox"
				size="40" />
			
			<field name="return_route_id" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURN_ROUTEID" description="COM_BOOKPRO_PASSENGER_RETURN_ROUTEID" class="inputbox" size="40" />
			
			<field name="route_id" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_ROUTEID" description="COM_BOOKPRO_PASSENGER_ROUTEID" class="inputbox" size="40" />
			
			<field name="price" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_PRICE" description="COM_BOOKPRO_PASSENGER_PRICE" class="inputbox" size="40" />
			
			<field name="return_price" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURNPRICE" description="COM_BOOKPRO_PASSENGER_RETURNPRICE" class="inputbox" size="40" />
			
			<field name="return_start" type="date" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURNSTART" description="COM_BOOKPRO_PASSENGER_RETURNSTART" class="inputbox" size="40" />
			
			<field name="bag_qty" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_BAGQTY" description="COM_BOOKPRO_PASSENGER_BAGQTY" class="inputbox" size="40" />
			
			<field name="price_bag" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_PRICEBAG" description="COM_BOOKPRO_PASSENGER_PRICEBAG" class="inputbox" size="40" />
						
			<field name="return_bag_qty" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURNBAGQTY" description="COM_BOOKPRO_PASSENGER_RETURNBAGQTY" class="inputbox" size="40" />
			
			<field name="return_price_bag" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURN_PRICEBAG" description="COM_BOOKPRO_PASSENGER_RETURN_PRICEBAG" class="inputbox" size="40" />
			
			<field name="package" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_PACKAGE" description="COM_BOOKPRO_PASSENGER_PACKAGE" class="inputbox" size="40" />
			
			<field name="return_package" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_RETURNPACKAGE" description="COM_BOOKPRO_PASSENGER_RETURNPACKGE" class="inputbox" size="40" />
			
			<field name="parent_id" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_PARENT_ID" description="COM_BOOKPRO_PASSENGER_PARENT_ID" class="inputbox" size="40" />
			
			<field name="weight" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_WEIGHT" description="COM_BOOKPRO_PASSENGER_WEIGHT" class="inputbox" size="40" />
			
			<field name="co" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_CO" description="COM_BOOKPRO_PASSENGER_CO" class="inputbox" size="40" />
			
			<field name="co_weight" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_COWEIGHT" description="COM_BOOKPRO_PASSENGER_COWEIGHT" class="inputbox" size="40" />
			
			<field name="check_in" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_CHECK_IN" description="COM_BOOKPRO_PASSENGER_CHECK_IN" class="inputbox" size="40" />
			
			<field name="check_in_weight" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_CHECK_IN_WEIGHT" description="COM_BOOKPRO_PASSENGER_CHECK_IN_WEIGHT" class="inputbox" size="40" />
			
			<field name="notes" type="text" required="false"
				label="COM_BOOKPRO_ORDER_NOTE" description="COM_BOOKPRO_ORDER_NOTE" class="inputbox" size="40" />
			
			<field name="street" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_STREET" description="COM_BOOKPRO_PASSENGER_STREET" class="inputbox" size="40" />
			
			<field name="city" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_CITY" description="COM_BOOKPRO_PASSENGER_CITY" class="inputbox" size="40" />
			
			<field name="state_province_parish" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_STATE_PROVINCE_PARISH" description="COM_BOOKPRO_PASSENGER_STATE_PROVINCE_PARISH" class="inputbox" size="40" />
			
			<field name="nationality" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_NATIONALITY" description="COM_BOOKPRO_PASSENGER_NATIONALITY" class="inputbox" size="40" />
			
			<field name="country_of_residenc" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_COUNTRY_OF_RESIDENC" description="COM_BOOKPRO_PASSENGER_COUNTRY_OF_RESIDENC" class="inputbox" size="40" />
			
			<field name="issueby" type="text" required="false"
				label="COM_BOOKPRO_PASSENGER_ISSUEBY" description="COM_BOOKPRO_PASSENGER_ISSUEBY" class="inputbox" size="40" />
			
		</fields>
	</fieldset>
</form>	